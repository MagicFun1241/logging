{
  "name": "logging",
  "version": "4.0.0",
  "description": "Lightweight informative modern console logging.",
  "main": "./lib",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc -p .",
    "test": "jest && yarn -s run example",
    "lint": "eslint  './**/*.@(ts|tsx|js)' --f codeframe --fix && tsc -p .",
    "example": "ts-node examples/typescript-example.ts && node examples/complex-example.js",
    "prepublish": "yarn -s run build",
    "precommit": "yarn -s run test",
    "np": "np"
  },
  "repository": "http://github.com/dylang/logging",
  "author": "Dylan Greene <dylang@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "7.5.5",
    "@sindresorhus/is": "1.2.0",
    "ansi-hash": "^1.0.0",
    "boxen": "4.1.0",
    "chalk": "^2.4.2",
    "clean-stack": "^2.2.0",
    "fade-steps": "^1.1.0",
    "is-ci": "^2.0.0",
    "lodash.groupby": "^4.6.0",
    "log-symbols": "^3.0.0",
    "log-update": "^3.3.0",
    "min-indent": "^1.0.0",
    "nicely-format": "^1.1.0",
    "parse-ms": "^2.1.0",
    "pkg-dir": "^4.2.0",
    "pretty-format": "^24.9.0",
    "pretty-ms": "5.0.0",
    "round-to": "^4.0.0",
    "serialize-error": "5.0.0",
    "strip-indent": "^3.0.0",
    "term-size": "2.1.0",
    "time-span": "3.1.0",
    "trim-newlines": "3.0.0",
    "wrap-ansi": "6.1.0"
  },
  "devDependencies": {
    "@types/babel__code-frame": "7.0.1",
    "@types/common-tags": "1.8.0",
    "@types/eslint": "6.1.3",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/is-ci": "^2.0.0",
    "@types/lodash.groupby": "^4.6.6",
    "@types/jest": "^24.0.20",
    "@types/min-indent": "^1.0.0",
    "@types/node": "12.11.7",
    "@types/prettier": "1.18.2",
    "@types/wrap-ansi": "3.0.0",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "common-tags": "1.8.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-xo": "0.27.2",
    "eslint-config-xo-typescript": "^0.19.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "^24.9.0",
    "lynt": "^0.5.5",
    "np": "^5.1.1",
    "prettier": "1.18.2",
    "strip-ansi": "5.2.0",
    "ts-jest": "^24.1.0",
    "ts-node": "8.4.1",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
