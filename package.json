    "ansi-hash": "^1.0.0",
    "boxen": "4.1.0",
    "chalk": "^2.4.2",
    "clean-stack": "^2.1.0",
    "fade-steps": "^1.1.0",
    "is-ci": "^2.0.0",
    "lodash.groupby": "^4.6.0",
    "log-symbols": "^3.0.0",
    "log-update": "^3.2.0",
    "min-indent": "^1.0.0",
