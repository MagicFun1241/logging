{
  "name": "logging",
  "version": "4.0.0",
  "description": "Lightweight informative modern console logging.",
  "main": "./lib",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc -p .",
    "test": "jest && yarn -s run example",
    "lint": "tslint --project ./tsconfig.json --format codeFrame --fix",
    "example": "ts-node examples/typescript-example.ts && node examples/complex-example.js",
    "prepublish": "yarn -s run build",
    "precommit": "yarn -s run test",
    "np": "np"
  },
  "repository": "http://github.com/dylang/logging",
  "author": "Dylan Greene <dylang@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "7.0.0-beta.51",
    "@sindresorhus/is": "0.9.0",
    "ansi-hash": "^1.0.0",
    "boxen": "1.3.0",
    "chalk": "^2.4.1",
    "clean-stack": "^1.3.0",
    "fade-steps": "^1.1.0",
    "is-ci": "^1.1.0",
    "lodash.groupby": "^4.6.0",
    "log-symbols": "^2.2.0",
    "log-update": "^2.3.0",
    "min-indent": "^1.0.0",
    "nicely-format": "^1.1.0",
    "parse-ms": "^1.0.1",
    "pkg-dir": "^3.0.0",
    "pretty-format": "^23.0.1",
    "pretty-ms": "3.2.0",
    "round-to": "^3.0.0",
    "serialize-error": "2.1.0",
    "strip-indent": "^2.0.0",
    "term-size": "1.2.0",
    "time-span": "2.0.0",
    "trim-newlines": "2.0.0",
    "wrap-ansi": "3.0.1"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.0",
    "@types/clean-stack": "^1.3.0",
    "@types/common-tags": "^1.4.0",
    "@types/jest": "^23.1.1",
    "@types/lodash.groupby": "^4.6.3",
    "@types/log-symbols": "^2.0.0",
    "@types/log-update": "^2.0.0",
    "@types/node": "10.3.4",
    "@types/parse-ms": "^1.0.0",
    "@types/pkg-dir": "^2.0.0",
    "@types/pretty-format": "^20.0.0",
    "@types/pretty-ms": "^3.0.0",
    "@types/strip-ansi": "^3.0.0",
    "@types/time-span": "^2.0.0",
    "@types/wrap-ansi": "^3.0.0",
    "common-tags": "1.8.0",
    "jest": "^23.1.0",
    "lynt": "^0.3.1",
    "np": "^3.0.4",
    "strip-ansi": "4.0.0",
    "ts-jest": "^22.4.6",
    "ts-node": "6.1.1",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
